importPackage(java.io);

var fixtures_directory = vars.get(&quot;FIXTURES_DIRECTORY&quot;);
var filename = fixtures_directory + &quot;/fixture_&quot; + vars.get(&apos;pid&apos;) + &quot;_&quot; + vars.get(&apos;dsid&apos;);
var file = new RandomAccessFile(filename, &quot;rw&quot;);

// Generate a file with a length of a suitable deviation from the mean.
var seed = parseInt(vars.get(&quot;SEED&quot;)) + parseInt(vars.get(&quot;seed_counter&quot;));
var random = new Random(seed);
var std_deviation_size = parseInt(vars.get(&quot;FILE_SIZE_STD_DEV&quot;));
var mean_size = parseInt(vars.get(&quot;FILE_SIZE_MEAN&quot;));
var size = -1;
while(size &lt; 0) {
  size = random.nextGaussian() * std_deviation_size + mean_size;
}
file.setLength(size);
// Test with less than 4 bytes may be off.
if((size - 4) &gt; 0) { 
 var position = random.nextInt(size - 4);
 file.seek(position);
 file.writeInt(random.nextInt());
}
file.close();</stringProp>
