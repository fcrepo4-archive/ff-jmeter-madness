var random= new Random(vars.get(&apos;modify_datastream_counter&apos;));
// Test with less than 4 bytes may be off.
if((size - 4) &gt; 0) { 
 var position = random.nextInt(size - 4);
 file.seek(position);
 file.writeInt(1 + random.nextInt());
}
file.close();</stringProp>
